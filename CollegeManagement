import java.util.*;

public class CollegeManagement {

    public static void main(String[] args) {

        // 1ï¸âƒ£ List â€“ Store student names (order matters, duplicates allowed)
        List<String> students = new ArrayList<>();
        students.add("Ravi");
        students.add("Sita");
        students.add("Kiran");
        students.add("Ravi"); // duplicate allowed

        // 2ï¸âƒ£ Set â€“ Store unique roll numbers (no duplicates)
        Set<Integer> rollNumbers = new HashSet<>();
        rollNumbers.add(101);
        rollNumbers.add(102);
        rollNumbers.add(103);
        rollNumbers.add(101); // duplicate ignored

        // 3ï¸âƒ£ Map â€“ Map roll number to student name
        Map<Integer, String> studentMap = new HashMap<>();
        studentMap.put(101, "Ravi");
        studentMap.put(102, "Sita");
        studentMap.put(103, "Kiran");

        // 4ï¸âƒ£ Queue â€“ Students waiting for fee payment (FIFO)
        Queue<String> feeQueue = new LinkedList<>();
        feeQueue.add("Ravi");
        feeQueue.add("Sita");
        feeQueue.add("Kiran");

        // 5ï¸âƒ£ Stack â€“ Undo operations (LIFO)
        Stack<String> actions = new Stack<>();
        actions.push("Add Student");
        actions.push("Update Marks");
        actions.push("Delete Record");

        // ğŸ”¹ Output
        System.out.println("Students List: " + students);
        System.out.println("Roll Numbers Set: " + rollNumbers);
        System.out.println("Student Map: " + studentMap);
        System.out.println("Fee Queue: " + feeQueue);

        System.out.println("Last Action (Undo): " + actions.pop());
    }
}
